{
  "name": "jettison",
  "version": "0.5.0",
  "description": "Encode JavaScript binary data into strings to send over the network.",
  "main": "./lib/jettison.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rimraf lib *.tgz",
    "compile": "coffee --compile --map --output lib src/*.coffee",
    "prepublish": "npm run compile",
    "postpack": "npm run clean",
    "postpublish": "npm run clean",
    "test": "mocha --compilers coffee:coffee-script/register"
  },
  "browserify": {
    "transform": [
      "coffeeify"
    ]
  },
  "author": "Nathan Ostgard",
  "license": "MIT",
  "dependencies": {
    "utf8": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^11.0.0",
    "chai": "^3.0.0",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "webpack": "^1.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noonat/jettison.git"
  },
  "bugs": {
    "url": "https://github.com/noonat/jettison/issues"
  },
  "homepage": "https://github.com/noonat/jettison",
  "keywords": [
    "binary",
    "pack",
    "struct",
    "jspack"
  ]
}
